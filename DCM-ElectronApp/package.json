{
  "name": "my-new-app",
  "productName": "my-new-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start-backend": "cd ../DCM-back && npm start",
    "start-frontend": "cd ../DCM-front-v3 && npm run dev",
    "start-electron": "electron-forge start",
    "start-database": "mongod --dbpath=data/",
    "start": "concurrently \"npm run start-database\" \"npm run start-backend\" \"npm run start-frontend\" \"npm run start-electron\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "Nico",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "28.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "electron-squirrel-startup": "^1.0.0",
    "mongod": "^2.0.0"
  }
}
